From 2cc6dc5fcfa0be604b5f1845d4eb60ae5dbd8584 Mon Sep 17 00:00:00 2001
From: Rok Mandeljc <rok.mandeljc@gmail.com>
Date: Mon, 14 Apr 2014 15:02:37 +0200
Subject: [PATCH] Removed HAVE_STDINT guards in flip1.c and flip2.c since

 they cause errors under MinGW
---
 flip1.c | 5 -----
 flip2.c | 5 -----
 2 files changed, 10 deletions(-)

diff --git a/flip1.c b/flip1.c
index 6d634ec..dfb5730 100644
--- a/flip1.c
+++ b/flip1.c
@@ -29,12 +29,7 @@
 #include <errno.h>
 #include <limits.h>
 #include <unistd.h>
-
-#if HAVE_STDINT_H
 #include <stdint.h>
-#elif HAVE_INTTYPES_H
-#include <inttypes.h>
-#endif
 
 
 #include "avrdude.h"
diff --git a/flip2.c b/flip2.c
index aca455c..f8801fd 100644
--- a/flip2.c
+++ b/flip2.c
@@ -26,12 +26,7 @@
 #include <errno.h>
 #include <limits.h>
 #include <unistd.h>
-
-#if HAVE_STDINT_H
 #include <stdint.h>
-#elif HAVE_INTTYPES_H
-#include <inttypes.h>
-#endif
 
 #include "avrdude.h"
 #include "libavrdude.h"
